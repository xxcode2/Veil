# Arcium MPC Configuration
# Copy this file to .env and fill in your values

# Your deployed Arcium program ID (58-character Solana address)
# Get this after running: arcium deploy
ARCIUM_PROGRAM_ID=YOUR_PROGRAM_ID_HERE

# Arcium API key from https://app.arcium.com
ARCIUM_API_KEY=your-api-key-here

# Network: devnet for testing, mainnet-beta for production
ARCIUM_NETWORK=devnet

# Arcium API endpoint
ARCIUM_ENDPOINT=https://api.arcium.com
